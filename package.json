{
  "name": "n8n-nodes-aisensy",
  "version": "0.1.0",
  "description": "n8n custom node for Aisensy API campaign operations",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "dev:watch": "concurrently \"tsc --watch\" \"gulp watch:icons\"",
    "dev:sync": "concurrently \"npm run dev:watch\" \"node watch-and-sync.js\"",
    "dev:start": "npm run build && npx n8n",
    "dev:full": "npm run build && concurrently \"npm run dev:watch\" \"npx n8n\"",
    "watch": "npm run dev:sync",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/AiSensy/AiSensy.node.js"
    ],
    "credentials": [
      "dist/credentials/AiSensyApi.credentials.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~5.45.0",
    "chokidar": "^3.5.3",
    "concurrently": "^9.2.1",
    "eslint-plugin-n8n-nodes-base": "~1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "n8n-nodes-base": "*",
    "n8n-workflow": "*"
  }
}
